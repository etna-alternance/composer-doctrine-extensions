{
    "name": "etna/doctrine-extensions",
    "description": "Doctrine Extensions",
    "keywords": ["traits", "doctrine", "orm", "extensions"],
    "license": "proprietary",
    "authors": [
        {
            "name": "ETNA",
            "email": "dev@etna-alternance.net",
            "homepage": "http://etna-alternance.net"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/orm": "2.x@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.2",
        "php-coveralls/php-coveralls": "^2.1",
        "phing/phing":"^2.16"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts" : {
        "phing": "phing",
        "coveralls": "coveralls",
        "post-install-cmd": [
            "mkdir -p build",
            "chmod 777 build"
        ]
    },
    "repositories": [

   ]
}
